{
  "name": "lerna-cra",
  "private": true,
  "scripts": {
    "start": "npm -C ./app run start",
    "bootstrap": "cross-env NODE_ENV=development  lerna bootstrap",
    "publish": "lerna publish --registry http://localhost:4873/",
    "test": "jest --config jest.config.json",
    "storybook": "start-storybook -p 9999 -s .storybook",
    "cover": "npm run test -- --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@storybook/addon-actions": "^4.0.8",
    "@storybook/addon-info": "^4.0.8",
    "@storybook/addon-knobs": "^4.0.8",
    "@storybook/addon-links": "^4.0.8",
    "@storybook/addon-options": "^4.0.8",
    "@storybook/addon-storyshots": "^4.0.8",
    "@storybook/core": "^4.0.8",
    "@storybook/react": "^4.0.8",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "highlight-loader": "^0.7.3",
    "jest": "^23.6.0",
    "lerna": "^3.3.1",
    "markdown-loader": "^4.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "storybook": "^1.0.0",
    "storybook-readme": "^4.0.2",
    "webpack": "^4.27.1"
  },
  "dependencies": {
    "babel-plugin-require-context-hook": "^1.0.0",
    "chalk": "^2.4.1",
    "npmlog": "^4.1.2",
    "path": "^0.12.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "save-dev": "^2.0.0",
    "shelljs": "^0.8.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
